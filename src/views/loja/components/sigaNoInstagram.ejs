<% if (typeof redesSociais != "undefined" && redesSociais.length > 0) { %>


    <% redesSociais.forEach(element => { %>
        <% if (element.nome.toLowerCase() == "instagram") { %>
            <section class="w-full aspect-video md:aspect-auto md:py-16 p-5 flex flex-col justify-center items-center space-y-2 md:space-y-4 bg-sigaNoInstagram">
                <div class="flex items-center justify-center space-x-2 text-2xl font-semibold">
                    <i class="bi bi-instagram"></i>
                    <p><%= element.perfil %></p>
                </div>
                <p >Estamos no instagram</p>
            
                <a href="<%= element.url %>" target="_blank" class="px-4 py-2  rounded-full   bg-c1 text-white ">SIGA-NOS</a>
            </section>
        <% } %>
     
    <% }) %>


<% } %>